<addons>
<addon id="plugin.program.tmb" name="[B]Trademark Wizard[/B]" version="1.1" provider-name="Titan Thanos">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>executable</provides>
  </extension>
  <extension point="xbmc.service" library="startup.py" start="startup" />
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Trademark Wizard</summary>
    <description lang="en">Please visit https://tmbkodi.com and click on some of the adverts to help with the server costs. Thank you.</description>
    <platform>all</platform>
	<forum>http://koditalk.org/</forum>
    <website>https://tmbkodi.com/</website>
  <assets>
	<icon>icon.png</icon>
	<fanart>fanart.jpg</fanart>
	<screenshot>resources/screenshots/screenshot1.jpg</screenshot>
    <screenshot>resources/screenshots/screenshot2.jpg</screenshot>
    <screenshot>resources/screenshots/screenshot3.jpg</screenshot>
	<screenshot>resources/screenshots/screenshot4.jpg</screenshot>
    <screenshot>resources/screenshots/screenshot5.jpg</screenshot>
  </assets>
  </extension>
</addon>
<addon id="plugin.video.oculus" name="Oculus" version="1.23" provider-name="Trademark">
